{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "light",
    "orientation": "portrait",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-image-picker",
      "expo-font"
    ],
    "ios": {
      "bundleIdentifier": "com.anonymous.frontend",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library so you can select an item picture.",
        "NSCameraUsageDescription": "We need access to your camera so you can take a picture of the item."
      }
    },
    "android": {
      "package": "com.anonymous.frontend",
      "permissions": [
        "READ_MEDIA_IMAGES"
      ]
    },
    "infoPlist": {
      "NSPhotoLibraryUsageDescription": "â€¦",
      "NSCameraUsageDescription": "We need access to your camera so you can snap a photo of the item."
    }
  }
}
